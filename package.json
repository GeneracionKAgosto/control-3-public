{
  "name": "control-3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env BABEL_ENV=dev babel src -x .ts -d public/js",
    "build:watch": "cross-env BABEL_ENV=dev babel src -x .ts -d public/js --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "hs public -o -c 0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/jest": "^23.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-import-rename": "^1.0.1",
    "cross-env": "^5.2.0",
    "http-server": "^0.11.1",
    "jest": "^23.5.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleDirectories": [
      "./node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ]
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {}
